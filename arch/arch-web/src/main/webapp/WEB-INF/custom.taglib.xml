<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
    version="2.0">
    <namespace>http://kmware.com/ui</namespace>

    <tag>
        <tag-name>formFieldsets</tag-name>
        <component>
            <component-type>com.kmware.UIFormFieldset</component-type>
            <renderer-type>com.kmware.FormFieldsetRenderer</renderer-type>
        </component>
    </tag>
     <tag>
        <tag-name>dataTable</tag-name>
        <source>tags/dataTable.xhtml</source>
        <attribute>
        	<display-name>bean</display-name>
        	<name>bean</name>
        </attribute>
    </tag>
    <tag>
        <tag-name>globalFilter</tag-name>
        <source>tags/globalFilter.xhtml</source>
        <attribute>
        	<display-name>for</display-name>
        	<name>for</name>
        	<type>java.lang.String</type>
        </attribute>
        <attribute>
        	<display-name>id</display-name>
        	<name>id</name>
        </attribute>
        <attribute>
        	<display-name>expression</display-name>
        	<name>expression</name>
        </attribute>
    </tag>
</facelet-taglib>